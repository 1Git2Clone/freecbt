{
  "name": "@freecbt/web",
  "description": "web version of the freecbt app",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "https://github.com/erosson/elm-application-template",
  "private": true,
  "scripts": {
    "start": "elm-app start",
    "build": "elm-app build",
